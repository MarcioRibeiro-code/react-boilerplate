{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "src/app.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev",
    "test": "yarn lint && istanbul cover node_modules/mocha/bin/_mocha -- --opts test/mocha.opts",
    "test:lite": "_mocha --opts test/mocha.opts",
    "test:watch": "nodemon --ext .js -w ./ --exec '_mocha --opts test/mocha.opts || true'",
    "dev": "NODE_ENV=development webpack-dev-server --hot --inline",
    "dist": "NODE_ENV=production webpack",
    "lint": "eslint './src/**/*.js' './test/**/*.js'"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.18",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "html-webpack-plugin": "^3.0.6",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.12",
    "react-test-renderer": "^16.2.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "styled-jsx": "^2.2.4",
    "styled-jsx-plugin-sass": "^0.2.2"
  }
}
